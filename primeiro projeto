print ("covid-19")
suspeitos = 0 
while True:
    tosse = int(input("tosse? \n1. Sim \n2. Nao \nResp.:"))
    febre = int(input("febre? \n1. Sim \n2. Nao \nResp.:"))
    resp =  int(input("falta de ar? \n1. Sim \n2. Nao \resp.:"))

    if tosse == 1 and febre == 1 and resp == 1:
       suspeitos += 1 

resp = input ("ainda há pacientes? [S|N]?")
if resp.upper() == "N":
    breakpoint

print ("supeitos de covid 19: {}". format (suspeitos))


